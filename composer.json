{
    "name": "icecat/icecat-integration",
    "description": "This plugin provides integration of pimcore with Icecat",
    "type": "pimcore-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Icecat NV",
        "email": "marketing@icecat.com",
        "homepage": "https://icecat.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "pimcore/pimcore": "^10.5",
        "dragonmantank/cron-expression": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "IceCatBundle\\": "src/"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "IceCatBundle\\IceCatBundle"
            ]
        }
    }
}